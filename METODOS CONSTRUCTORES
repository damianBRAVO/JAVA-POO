


public class ALUMNO {

    int id;
    String nombre ;
     String apellido ; 
     
     public Alumno(){
         
     }

    public ALUMNO(int id, String nombre, String apellido) {
        this.id = id;
        this.nombre = nombre;
        this.apellido = apellido;
    }
     
         
     }
     public void mostrarNombre(){
         System.out.println("Hola, soy un alumno y se decir mi nombre");
     }
